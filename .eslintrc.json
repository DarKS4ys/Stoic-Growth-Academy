{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "object-shorthand": "off",
        "no-process-exit":"off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "commonjs": true
    }
}
